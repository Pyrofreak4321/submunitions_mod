<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
		  <li>Rimefeller</li>
		</mods>
		<match Class="PatchOperationSequence">
			<success>Always</success>
			<operations>
				<li Class="PatchOperationReplace">
          <xpath>*/ThingDef[defName="Shell_Napalm"]/costList</xpath>
          <value>
              <costList>
                <Steel>20</Steel>
                <Napalm>100</Napalm>
              </costList>
          </value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>